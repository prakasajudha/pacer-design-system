{
  "name": "@pacer-ui/storybook-vue",
  "version": "1.0.0",
  "private": true,
  "description": "Storybook for PACER Vue components",
  "scripts": {
    "dev": "storybook dev -p 6007",
    "storybook": "storybook dev -p 6007",
    "build": "storybook build",
    "build:with-deps": "cd ../.. && pnpm --filter @pacer-ui/tokens build && pnpm --filter @pacer-ui/tailwind-preset build && pnpm --filter @pacer-ui/vue build && cd packages/storybook/vue && pnpm run build",
    "clean": "rm -rf storybook-static"
  },
  "dependencies": {
    "@pacer-ui/vue": "workspace:*",
    "vue": "^3.4.15"
  },
  "devDependencies": {
    "@pacer-ui/tailwind-preset": "workspace:*",
    "@storybook/addon-docs": "^8.6.15",
    "@storybook/addon-essentials": "^8.6.15",
    "@storybook/addon-links": "^8.6.15",
    "@storybook/blocks": "^8.6.15",
    "@storybook/testing-library": "^0.2.2",
    "@storybook/vue3": "^8.6.15",
    "@storybook/vue3-vite": "^8.6.15",
    "@vitejs/plugin-vue": "^5.0.3",
    "autoprefixer": "^10.4.0",
    "postcss": "^8.4.0",
    "postcss-import": "^16.1.1",
    "storybook": "^8.6.15",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.11"
  }
}
