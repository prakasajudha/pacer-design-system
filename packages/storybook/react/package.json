{
  "name": "@pacer-ui/storybook-react",
  "version": "1.0.1",
  "private": true,
  "description": "Storybook for PACER React components",
  "scripts": {
    "dev": "storybook dev -p 6006",
    "storybook": "storybook dev -p 6006",
    "build": "storybook build && node -e \"require('fs').copyFileSync(require('path').join(process.cwd(),'serve.json'), require('path').join(process.cwd(),'storybook-static','serve.json'))\"",
    "build:with-deps": "cd ../.. && pnpm --filter @pacer-ui/tokens build && pnpm --filter @pacer-ui/tailwind-preset build && pnpm --filter @pacer-ui/react build && pnpm --filter @pacer-ui/storybook-react build",
    "clean": "rm -rf storybook-static"
  },
  "dependencies": {
    "@pacer-ui/react": "workspace:*",
    "@pacer-ui/tokens": "workspace:*",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@pacer-ui/tailwind-preset": "workspace:*",
    "@storybook/addon-docs": "^8.6.15",
    "@storybook/addon-essentials": "^8.6.15",
    "@storybook/addon-links": "^8.6.15",
    "@storybook/blocks": "^8.6.15",
    "@storybook/react": "^8.6.15",
    "@storybook/react-vite": "^8.6.15",
    "@storybook/testing-library": "^0.2.2",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "autoprefixer": "^10.4.0",
    "postcss": "^8.4.0",
    "postcss-import": "^16.1.1",
    "storybook": "^8.6.15",
    "tailwindcss": "^3.4.0",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.3.3",
    "vite": "^5.0.11"
  }
}
